<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2>Chat</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <i id="username"></i> <input id="chatbox" class="form-control">
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div id="logs" class="mt-3">
                {{#each user}}
                <p><strong>{{this.email}}:</strong> {{this.message}}</p> 
                {{/each}}
            </div>
        </div>
    </div>
</div>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/static/js/index.js"></script>
